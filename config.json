{
  "name": "vih/sport_2014",
  "description": "Theme for vih.dk",
  "browserSyncProxy" : "http://panopoly.dev",
  "bowerDir": "./bower_components",
  "css": {
    "file" : "sport_2014.style.scss",
    "src": [
      "scss/**/*.scss"
    ],
    "dest": "assets/css",
    "includePaths": [
      "./bower_components/bootstrap-sass/assets/stylesheets",
      "./bower_components/font-awesome/scss",
      "./bower_components/compass-mixins/lib"
    ]
  },
  "js": {
    "file" : "sport_2014.script.js",
    "src": [
      "js/**/*.js"
    ],
    "dest": "assets/js"
  },
  "images": {
    "src": [
      "assets/images/**/*"
    ],
    "dest": "assets/images/"
  },
  "fonts": {
    "src": [
      "./bower_components/font-awesome/fonts/**.*"
    ],
    "dest": "./assets/fonts/"
  }
}
